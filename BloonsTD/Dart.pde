public class Dart{
  // Fields
  int size;
  double speed;
  float x;
  float y;
  
  public Dart(int biggy, double IAmSpeed, float floatX, float floatY) {
    size = biggy;
    speed = IAmSpeed;
    x = floatX;
    y = floatY;
  }
  
  // Accessors
  public float getX() {
    return x; 
  }
  public float getY() {
    return y; 
  }

}
