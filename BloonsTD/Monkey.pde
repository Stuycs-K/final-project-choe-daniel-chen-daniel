public class Monkey{
  // Fields
  private int cost;
  private int range;
  private int damage;
  private int placementRadius;
  private int reload;
  private float x;
  private float y;
  private boolean land;
  private boolean water;
  private boolean seeCamo;
  
  // Constructor
  public Monkey(int money, int area, int attack, int placement, int load, float placeX, float placeY, boolean landed, boolean watered, boolean camo) {
    cost = money;
    area = range;
    damage = attack;
    placementRadius = placement;
    reload = load;
    x = placeX;
    y = placeY;
    land = landed;
    water = watered;
    seeCamo = camo;
  }
  
  // Accessors 
  public int getCost() {
    return cost;
  }
  public float getX() {
    return x; 
  }
  public float getY() {
    return y; 
  }
  
